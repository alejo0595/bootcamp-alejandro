{% comment %}
  Parameters:
  - name {string} [required] - The icon name to render
  - color {string} [required] - The color to fill the icon
{% endcomment %}

{%-liquid
  if color
    assign fill_color = color
  else
    assign fill_color= 'currentColor'
  endif
  -%}

{%- case name -%}
  {%- when 'svg-beer' -%}
    {% render 'svg-beer' color: fill_color %}
  {%- when 'svg-bookmark' -%}
    {% render 'svg-bookmark' color: color %}
